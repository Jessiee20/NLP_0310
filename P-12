import nltk
from nltk import CFG
from nltk.parse.earleychart import EarleyChartParser
grammar = CFG.fromstring("""
S -> NP VP
NP -> Det N
VP -> V NP
Det -> 'the' | 'a'
N -> 'dog' | 'cat'
V -> 'chased' | 'saw'
""")
parser = EarleyChartParser(grammar)
sentence = "the dog chased a cat".split()
print("Parse Trees:")
for tree in parser.parse(sentence):
    print(tree)
